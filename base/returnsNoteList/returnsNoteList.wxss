/* base/returnsNoteList/returnsNoteList.wxss */
.content {
  min-height: 100vh;
  line-height: 1.414;
  color: #747474;
}
.ui-group {
  margin-bottom: 24rpx;
}

.ui-list {
  position: relative;
  color: #444;
  display: block;
  padding: 30rpx 24rpx 30rpx 0;
  margin-left: 24rpx;
  border-bottom: 1px solid #eee;
  word-break: break-all;
}
.ui-group-full .ui-list-bb {
  border-bottom: 1px solid #eee;
}
.ui-title {
  text-align: left;
  line-height: 50rpx;
  font-size: 32rpx;
}
.ui-content {
  color: #999;
  line-height: 40rpx;
  margin-top: 10rpx;
}