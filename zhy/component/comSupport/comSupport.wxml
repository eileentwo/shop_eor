<!--底部技术支持 -->
<view class='zhichi' wx:if="{{info.jszc_show == 1}}">
  <view class='logob_box'>
    <image class='logo_b' src="{{root + info.jszc_img}}" wx:if="{{(info.jszc_img && root)}}"></image>
  </view>
  <view class='center_word'>
    <view class='logo_name' wx:if="{{info.jszc_name}}">{{info.jszc_name}}</view>
    <view wx:if="{{info.jszc_tel}}">合作电话:{{info.jszc_tel}}</view>
  </view>
  <view class='callphone' bindtap="_onCallTab" wx:if="{{info.jszc_tel}}">拨打</view>
</view>