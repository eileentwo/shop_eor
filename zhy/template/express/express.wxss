/*物流*/
.logistics-mask{position: fixed; z-index: 12; top:0; right:0; bottom:0; left: 0; background: rgba(0,0,0,0.3)}
.logistics{position: fixed; z-index: 13; left: 5vw; top:5vh; width:90vw; height:90vh; background: #fff; }
.log-tit{ height:10vh; text-align: center; line-height: 10vh; font-size: 32rpx; font-weight: bold}
.log-con{ padding:0 30rpx 30rpx 30rpx; height:72vh; box-sizing: border-box; overflow-y:scroll; font-size: 28rpx;}
.log-con-close{ height:8vh; text-align: center; line-height: 8vh; box-sizing: border-box; border-top: 1rpx solid #ddd; font-size: 28rpx; color: #999}
.log-con-main{ margin-top: 30rpx;}
.log-con-main .v3{ margin-bottom: 15rpx; font-size: 28rpx; font-weight: bold}
.log-con-main .v4{ margin:0 0 15rpx 20rpx; font-size: 26rpx; color: #999}
.log-con-main .v5{ margin:0 0 15rpx 20rpx; font-size: 22rpx; color: #666}
.log-con-main .v6{ margin:20rpx 0 10rpx 20rpx; font-size: 28rpx; color: #222}
.log-con-head{ font-size: 24rpx}
/*物流-End*/